  </servlet>
  
  <servlet-mapping>
    <servlet-name>Profile</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <servlet-name>Session</servlet-name>
    <servlet-class>com.revature.p1.servlets.SessionServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Session</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>
  
</web-app>